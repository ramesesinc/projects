		                android:textSize="17sp" />
		
		        </RelativeLayout>

		         
		         <RelativeLayout
		            android:layout_width="match_parent"
		            android:layout_height="wrap_content" >
		
		            <TextView
		                android:id="@+id/TextView7"
		                android:layout_width="100dp"
		                android:layout_height="wrap_content"
		                android:layout_alignParentLeft="true"
