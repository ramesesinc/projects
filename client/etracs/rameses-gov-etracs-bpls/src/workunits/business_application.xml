<workunit schemaName="business_application">

    <invokers>
        <invoker type="business_application:open" 
                 caption="Business Application" 
                 formId="#{entity.objid}"
                 formTitle="#{entity.appno} - #{entity.apptype} - #{entity.business.businessname}"
                 action="open" target="window" 
        />
        
        <invoker type="global:barcode:51005" caption="Business Application" 
                 action="openBarcode" 
                 formId="#{entity.objid}"
                 formTitle="#{entity.appno} - #{entity.apptype} - #{entity.business.businessname}"
                 target="window" expr="#{barcodeid.contains('-')}" 
        />
        
       
