<workunit schemaName="entitymultiple">
    <invokers>
        <invoker type="entitymultiple:open" caption="Multiple Entity" action="open"  
                 role="MASTER" windowTitle="Entity: #{entity.entityno}" index="-1" 
                 formTitle="#{entity.entityno}" formId="#{entity.objid}"/>

        <invoker type="entitymultiple:create" caption="Multiple Entity" action="create"  
                 role="MASTER" windowTitle="New Multiple Entity" index="-1" 
                 formTitle="New Multiple Entity"/>        


        <invoker type="multipleentity:open" caption="Multiple Entity" action="open"  
                 role="MASTER" windowTitle="Entity: #{entity.entityno}" index="-1" 
                 formTitle="#{entity.entityno}" formId="#{entity.objid}"/>

        <invoker type="multipleentity:create" caption="Multiple Entity" action="create"  
                 role="MASTER" windowTitle="New Multiple Entity" index="-1" 
                 formTitle="New Multiple Entity"/>                     
    </invokers>
    <code class="com.rameses.entity.models.MultipleEntityModel" />
    <pages>
       <page template="com.rameses.entity.views.MultipleEntityPage"/>
    </pages>   
</workunit>