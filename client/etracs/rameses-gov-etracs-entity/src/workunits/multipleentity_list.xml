<workunit extends="wtemplates/ListController.xml" serviceName="MultipleEntityService"
    entityName="multipleentity" allowCreate="false"  allowOpen="false" orderBy="entityname">
    
    <invokers>
        <invoker folderid="/explorer/master/entity" caption="Multiple Ownership" role="MASTER" permission="multipleentity.view" />
        <invoker type="formActions" caption="New" action="create" role="MASTER" permission="multipleentity.create" mnemonic="n" shortcut="ctrl N"/>
        <invoker type="formActions" caption="Open" action="open" role="MASTER" permission="multipleentity.open" />
   </invokers>
</workunit>