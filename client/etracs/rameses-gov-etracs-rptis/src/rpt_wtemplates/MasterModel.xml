<workunit>
    <invokers>
        <invoker type="formActions" 
                 caption="Approve"
                 action="approve"
                 role="MASTER"
                 permission="master.approve" 
                 visibleWhen="#{allowApprove == true &amp;&amp; entity.state!='APPROVED'}" />
        
        <invoker type="formActions" 
                 caption="Disapprove"
                 action="disapprove"
                 role="MASTER"
                 permission="master.disapprove" 
                 visibleWhen="#{allowDisapprove == true  &amp;&amp; entity.state=='APPROVED'}" />
    </invokers>

    <code class="com.rameses.gov.etracs.rptis.master.models.MasterModel"/>
    
    <pages>
        <page template="com.rameses.gov.etracs.rptis.master.views.DefaultMasterPage"/>
    </pages>
</workunit>