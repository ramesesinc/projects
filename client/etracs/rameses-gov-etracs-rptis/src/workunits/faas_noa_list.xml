<workunit extends="com/rameses/seti2/templates/CrudList.xml"
    schemaName="vw_assessment_notice"
    entitySchemaName="assessmentnotice"
    allowDelete="false"
    allowPrint="false"
    allowCreate="false"
    autoResize="false"
    cols="state,txnno,txndate,tdno,fullpin,cadastrallotno,taxpayername,deliverydays,receivedby,dtdelivered,remarks"
    orderBy="txndate desc,tdno"
>
    <invokers>
        <invoker folderid="/home/rpt" 
            action="init"
            caption="Notice of Assessments"  
            target="window" index="500" role="ASSESSOR_SHARED,SHARED" 
            icon="home/icons/notices.png" />

        <invoker type="quick_launcher" code="NOA00" 
            action="init"
            caption="Notice of Assessments" 
            role="ASSESSOR_SHARED,SHARED" />
    </invokers>
    
</workunit>
