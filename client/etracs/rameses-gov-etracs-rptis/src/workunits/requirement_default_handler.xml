<?xml version="1.0" encoding="UTF-8"?>
<workunit>
    
    <invokers>
        <invoker type="rptrequirementtype:handler" action="init" name="default" caption="Default Handler" />
        
        <invoker type="formActions" action="complied" 
            caption="Complied" 
            mnemonic="c"  
            role="RECEIVER,EXAMINER" 
            visibleWhen="#{mode == 'read' &amp;&amp; !caller?.entity.state.matches('CURRENT|CANCELLED') &amp;&amp; !entity.complied}"/>

        <invoker type="formActions" action="uncomplied" 
            caption="Uncomply" 
            mnemonic="u"  
            role="RECEIVER,EXAMINER" 
            visibleWhen="#{mode == 'read' &amp;&amp; !caller?.entity.state.matches('CURRENT|CANCELLED') &amp;&amp; entity.complied}"/>

        <invoker type="formActions" action="deleteTxnAttachment" 
            caption="Delete Attachment" 
            role="RECEIVER,EXAMINER" 
            visibleWhen="#{mode == 'read' &amp;&amp; !caller?.entity.state.matches('CURRENT|CANCELLED')}"/>
    </invokers>
        
    <code class="com.rameses.gov.etracs.rptis.models.RPTRequirementTypeDefaultHandlerModel" >
    </code>
    
    <pages>
        <page template="com.rameses.gov.etracs.rptis.master.views.RequirementTypeDefaultHandlerPage" />
        <page name="complied" template="com.rameses.gov.etracs.rptis.master.views.RequirementTypeCompliedPage" />
    </pages>
</workunit>

