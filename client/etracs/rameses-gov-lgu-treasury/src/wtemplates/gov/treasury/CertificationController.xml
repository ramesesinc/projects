<?xml version="1.0" encoding="UTF-8"?>
<workunit>
    <invokers>
        <!-- default -->
        <invoker type="formActions" action="_close"   caption="Cancel"    shourtcut="c" mnemonic="c"  index="0"  visibleWhen="#{mode == 'create'}" immediate="true" icon="images/toolbars/cancel.png" />
        <invoker type="formActions" action="_close"   caption="Close"    shourtcut="c" mnemonic="c"  index="0"  visibleWhen="#{mode != 'create'}" immediate="true" icon="images/toolbars/cancel.png" />
        <invoker type="formActions" action="doSave"     caption="Save"      shourtcut="s" mnemonic="s"  index="2"  visibleWhen="#{mode == 'create'}" icon="images/toolbars/save.png" />        
    </invokers>
    <pages>
        <page template="com.rameses.gov.treasury.certification.CertificationPage" />
        <page name="preview" template="com.rameses.gov.treasury.report.PreviewPage" />
    </pages>
</workunit>
