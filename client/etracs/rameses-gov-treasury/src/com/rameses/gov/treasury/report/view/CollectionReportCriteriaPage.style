"entity.*" [1==1] {
    visible: false; 
}
"entity.(period|collector)" [1==1] {
    visible: true; 
}
"entity.year" [entity.period.toString().matches('monthly')==true] {
    visible: true; 
}
"entity.month" [entity.period.toString().matches('monthly')==true] {
    visible: true; 
}
"entity.date" [entity.period.toString().matches('daily')==true] {
    visible: true; 
}

