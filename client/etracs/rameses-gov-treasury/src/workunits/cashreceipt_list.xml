<workunit extends="com/rameses/seti2/templates/CrudNodeList.xml" 
          schemaName="cashreceipt" allowCreate="false" allowDelete="false" 
          cols="formno,receiptno,receiptdate,stub,paidby,amount,voided,collector.name">
    
    <invokers>
        <invoker folderid="menucategory/treasury/mgmt" caption="Cash Receipt List" action="init"
                 customFilter=" formtype='serial' "
                 role="SHARED" tag="mgmt"/>

        <invoker folderid="menucategory/treasury/collection" caption="Collector Cash Receipts" 
                 customFilter=" formtype='serial' "
                 role="COLLECTOR"
                 tag="collector"
                 action="init" index="50" /> 
        
        <invoker folderid="menucategory/treasury/collection" caption="Sub Collector Cash Receipts" 
                 customFilter=" formtype='serial' "
                 role="SUBCOLLECTOR"
                 tag="subcollector"
                 action="init" index="50" /> 
           
        
        
        <invoker type="cashreceipt_list:afseries" caption="Cash Receipt List" action="init" 
                 customFilter=" controlid = :afcontrolid AND series BETWEEN :fromseries AND :toseries"
                 orderBy="receiptno"
                 target="popup" width="1024" height="700"/>
   </invokers>
   
</workunit>