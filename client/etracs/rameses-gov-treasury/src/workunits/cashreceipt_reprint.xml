<workunit schemaName="cashreceipt_reprint_log">
    <invokers>
        <!-- hooks to cash receipt -->
        <!--
        <invoker 
           type="cashreceiptinfo:formActions" caption="Reprint" action="reprintUnsecured" 
           target="popup"> 
        </invoker>
       -->
       
        <invoker 
           type="cashreceipt:form:formActions" caption="Reprint" action="reprintSecured" 
           role="COLLECTOR,SUBCOLLECTOR" 
           applySecurity="true" 
           target="popup"> 
        </invoker>   
        
        <invoker 
           type="cashreceipt:reprint" caption="Reprint" action="reprintUnsecured" target="popup"> 
