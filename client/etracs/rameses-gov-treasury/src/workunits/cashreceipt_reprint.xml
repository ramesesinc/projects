    </invokers>
   
    <code class="com.rameses.enterprise.treasury.models.CashReceiptReprintModel" />
   
    <pages>
        <page template="com.rameses.enterprise.treasury.views.SupervisorApprovalPage"/>
    </pages>
   
</workunit>