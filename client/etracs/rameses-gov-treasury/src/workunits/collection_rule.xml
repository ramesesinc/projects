<workunit>
    <invokers>
        <invoker type="collection_rule" caption="Ask Info" action="init" target="popup"/>
        
        <!-- Form Actions -->
        <invoker type="formActions" caption="Cancel" action="doCancel" shortcut="ctrl C" immediate="true"/>
        <invoker type="formActions" caption="Back" action="doBack" visibleWhen="#{hasBack == true}" mnemonic="B" immediate="true"/>
        <invoker type="formActions" caption="Next" action="doNext" mnemonic="N" shortcut="ctrl N"/>
    </invokers>
    <code class="com.rameses.enterprise.treasury.models.CollectionRuleModel"/>
    <pages>
        <page template="com.rameses.treasury.common.views.AskInfoPage"/>
    </pages>
</workunit>