<workunit extends="com/rameses/seti2/templates/CrudLookup.xml" schemaName="collectiontype">
    <invokers>
        <invoker type="collectiontype:lookup" 
                 caption="Lookup Collection Types" 
                 action="init"  
                 cols="objid,name,title,formno,handler,barcodekey"/>
        
        <invoker type="collectiontype:paymentorder:lookup" 
                 caption="Lookup Collection Type for Payment Order" 
                 action="init"  
                 customFilter="allowpaymentorder = 1"
                 cols="objid,name,title"/>
        
         <invoker type="collectiontype:kiosk:lookup" 
                 caption="Lookup Collection Type for Kiosk" 
                 action="init"  
                 customFilter="allowkiosk = 1"
                 cols="objid,name,title"/>
        
    </invokers>
</workunit>