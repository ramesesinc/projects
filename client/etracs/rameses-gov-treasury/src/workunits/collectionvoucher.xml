<workunit extends="com/rameses/seti2/templates/CrudForm.xml" schemaName="collectionvoucher" 
          allowCreate="false" allowEdit="false">
    <invokers>
        <invoker type="collectionvoucher:open" 
                 caption="Collection Voucher" 
                 windowTitle="#{entity.controlno}"
                 formTitle="Collection Voucher #{entity.controlno}"
                 formId="#{entity.objid}"
                 action="open" target="window" 
                 index="10"/>
        
        <!-- Buttons -->
        <invoker type="formActions" caption="Change Liq Officer" action="changeLiqOfficer" 
                 visibleWhen="#{entity.state == 'OPEN'}" 
                 role="ADMIN"/>
        <invoker type="formActions" caption="Post" action="post" visibleWhen="#{entity.state == 'OPEN'}"/>
        
        
    </invokers>
   
   <code class="com.rameses.enterprise.treasury.models.CollectionVoucherModel" />
  
   <pages>
       <page template="com.rameses.enterprise.treasury.views.CollectionVoucherPage" />
   </pages>
   
</workunit>