<workunit schemaName="fund">
        
    <invokers>
         <invoker type="fund:create" action="create" caption="Fund (New)" role="MASTER" permission="fund.create" target="popup"/>
         <invoker type="fund:open" action="open" caption="Fund" role="MASTER" permission="fund.open" target="popup"/>
         
         <invoker type="quick_launcher" code="FUND01" caption="Fund" role="MASTER" permission="fund.create"/>
          
         <invoker type="formActions" caption="Activate" action="activate" visibleWhen="#{entity.state.matches('DRAFT|INACTIVE')}"/>
         <invoker type="formActions" caption="Deactivate" action="deactivate" visibleWhen="#{entity.state == 'ACTIVE'}"/>

    </invokers>
   
   <code class="com.rameses.enterprise.treasury.models.FundModel" />
   
   <pages>
       <page template="com.rameses.enterprise.treasury.views.FundPage"/>
   </pages>
   
</workunit>