<workunit extends="com/rameses/seti2/templates/CrudLookup.xml" schemaName="paymentcheck" allowDelete="false">
    <invokers>
       
        <invoker type="paymentcheck_split:lookup" caption="Lookup Cash Receipt" 
                 action="init" index="10" target="popup" 
                 cols="refno,receivedfrom,bank.name,refdate,balance"
                 hiddenCols="bank.objid"
                    customFilter=" amount - amtused > 0 "/>
        
        <invoker type="paymentcheck_undeposited:lookup" caption="Lookup Undeposited Cash Receipt" 
                 action="init" index="10" target="popup" 
                 cols="refno,receivedfrom,bank.name,refdate,amount"
                 hiddenCols="bank.objid" multiSelect="true"
                    customFilter=" bankdepositid IS NULL "/>
        
         <invoker type="paymentcheck:depositfund:lookup" caption="Lookup Undeposited Cash Receipt" 
                 action="init" index="10" target="popup" 
                 cols="refno,receivedfrom,bank.name,refdate,amount"
                 hiddenCols="bank.objid" multiSelect="true"
                    customFilter=" bankdepositid IS NULL "/>
    </invokers>
    
    <code>
        class PaymentCheckLookup extends com.rameses.seti2.models.CrudLookupModel {
        
        }
    </code>    
</workunit>