<workunit extends="com/rameses/seti2/templates/CrudNodeList.xml" 
          schemaName="remittance" allowCreate="false">
    <invokers>
        <invoker folderid="menucategory/treasury/remittance" 
                 caption="View Remittances" 
                 windowTitle="Remittances"
                 formTitle="Remittances"
                 cols="controlno,controldate,collector.name,liquidatingofficer.name,amount,state"
                 orderBy="controlno"
                 action="init" target="window" index="10"/>
        
        <invoker folderid="menucategory/treasury/collection" 
                 caption="View Remittances" 
                 windowTitle="Remittances By Collector"
                 formTitle="Remittances By Collector"
                 cols="controlno,controldate,collector.name,liquidatingofficer.name,amount,state"
                 orderBy="controlno"
                 role="COLLECTOR"
                 customFilter="collector.objid = :userid"
                 action="init" target="window" index="250"/>
    </invokers>
</workunit>