<workunit extends="com/rameses/seti2/templates/CrudList.xml" schemaName="cashreceipt"
    cols="formno,receiptno,txnmode,receiptdate,paidby,collectiontype.handler,collector.name,voided,amount" 
    orderBy="receiptdate desc">
    <invokers>
        <invoker folderid="/explorer/txn/treasury" caption="Unremitted Collections" role="COLLECTOR,SUBCOLLECTOR"  action="init" orderBy="formno,receiptno"/>
        <invoker folderid="menucategory/treasury/monitor" caption="Unremitted Collections" role="COLLECTOR,SUBCOLLECTOR" 
                 action="init" target="window" index="10" orderBy="formno,receiptno"/>
        
        <invoker type="unremitted_collection:collector:view" caption="Unremitted Collections" role="COLLECTOR" 
                 action="init" target="window" index="10"/>
        
        <!-- actions -->
        <invoker type="listActions" action="remit" caption="Remit" role="COLLECTOR" mnemonic="c" shortcut="ctrl T"/>   
        <invoker type="listActions" action="viewSum" caption="View Summary" />      
    </invokers>
    
    <code class="com.rameses.enterprise.treasury.models.UnremittedCollectionModel" />
</workunit>