        <page name="business" template="com.rameses.gov.etracs.market.views.MarketChangeActionPage" />
        <page name="ledger" template="com.rameses.gov.etracs.market.views.MarketChangeActionPage" />
                
        <page name="electricity" template="com.rameses.gov.etracs.market.views.ChangeElectricityReadingPage" />
