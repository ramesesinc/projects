<workunit>
    <invokers>
        <invoker type="formActions" caption="Assess" action="assess" 
                 visibleWhen="#{task?.state == 'reassessment' &amp;&amp; task?.assignee?.objid == user.objid }"/>
        
        <invoker type="formActions" caption="View Tracking No" action="viewTrackingno" />  
        
        <invoker type="formActions" caption="Print Permit" target="self" action="printPermit"
                 visibleWhen="#{ entity.permit?.objid != null }"
                 role="LICENSING" />  
    </invokers> 
    <code class="com.rameses.gov.etracs.vehicle.models.VehicleApplicationForm"/>
    <pages>
        <page template="com.rameses.gov.etracs.vehicle.views.ApplicationFormPage" />
    </pages>
</workunit>

