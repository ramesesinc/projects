<workunit>
    <code class="com.rameses.gov.etracs.vehicle.models.VehicleApplicationEntryForm" />
    <pageflow>
        <start>
            <transition to="search" action="setUp"/>
        </start>
        <page name="search" title="#{formTitle} -  Search Franchise No">
            <transition to="initial" caption="Next" action="open" immediate="false"/>
        </page>
        <page name="initial" title="#{formTitle} - Cancel Franchise">
            <transition to="confirm" caption="Next" immediate="false"/>
        </page>
        <page name="confirm" title="#{formTitle} - Verify Information">
            <transition to="assessment" caption="Back" />
            <transition to="end" caption="Submit" action="save" 
                confirm="You are about to drop this franchise. Proceed?"  immediate="false"/>
        </page>
        <end/>
    </pageflow>
    <pages>
        <page name="search" template="com.rameses.gov.etracs.vehicle.views.AskVehicleFranchiseNoPage" />
        <page name="success" template="com.rameses.gov.etracs.vehicle.views.ApplicationSuccessPage" />
    </pages>
</workunit>