"entity.*" [ entity.apptype.matches('RENEW|DROP') == true ] {
    enabled:false;
}

"entity.*" [ entity.apptype.matches('RENEW|DROP') != true ] {
    enabled:true;
}

"entity.appdate" [ entity.txnmode != 'CAPTURE' ] {
    enabled: false;
}

"entity.appdate" [ entity.apptype == 'LATE_RENEWAL' ] {
    enabled: true;
}

"entity.particulars" [1==1] {
    enabled: true;
}