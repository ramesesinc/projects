<workunit extends="com/rameses/seti2/templates/CrudLookup.xml" schemaName="entity">
    <invokers>
        <invoker type="entity:lookup" 
                 caption="Lookup Entity" action="init" 
                 target="popup" height="650" width="800" 
                 cols="entityno,name,address.text,type"
                 hiddenCols="entityname,address.objid"
                 customFilter=" state='ACTIVE' "
                 orderBy="entityname" 
                 surroundSearch="false" />
    </invokers>
    
    <code class="com.rameses.entity.models.EntityLookupModel"/>
    
    <pages> 
        <page name="queryForm" template="com.rameses.entity.views.EntityLookupQueryForm"/>
    </pages> 
</workunit>