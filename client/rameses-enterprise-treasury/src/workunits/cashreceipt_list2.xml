<workunit extends="com/rameses/seti2/templates/CrudNodeList.xml" 
          schemaName="cashreceipt" 
          allowCreate="false" allowEdit="false" allowDelete="false">
    
    <invokers>
        <invoker folderid="/home/collection" caption="Cash Receipts" 
                 role="SHARED" permission="cashreceipt.list" action="init" 
                 cols="formno,receiptno,txnmode,receiptdate,paidbyname,collector.name,collectiontype.name,void.objid,amount" />
        
        <!--
        <invoker type="formActions" caption="Revert Void" action="revertVoid" 
                 role="ADMIN" permission="cashreceiptvoid.revert" 
                 visibleWhen="#{allowRevertVoid==true}"/>
        -->         
   </invokers>
        
    
    <pages>
        <page name="queryform" template="com.rameses.enterprise.treasury.views.CashReceiptQueryFilter"/>
    </pages>        
</workunit>