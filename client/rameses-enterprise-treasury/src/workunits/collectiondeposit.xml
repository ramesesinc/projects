<workunit>
    <invokers>
        <invoker type="collectiondeposit:create" caption="New Cash Deposit" action="create"/> 
        
        <invoker type="formActions" caption="Close" name="_close"  immediate="true" mnemonic="c" shortcut="ctrl C" icon="images/toolbars/cancel.png"/>
        <invoker type="formActions" caption="Post" name="post" visibleWhen="#{entity.state=='DRAFT'}" mnemonic="s" shortcut="ctrl S" icon="images/toolbars/save.png"/>
        <invoker type="formActions" caption="Delete" action="delete" visibleWhen="#{ entity.state=='DRAFT' }" />        
   </invokers>

   <code class="com.rameses.enterprise.treasury.models.CollectionDepositModel" />

   <pages>
       <page template="com.rameses.enterprise.treasury.views.CollectionDepositViewPage" />
   </pages>
</workunit>
