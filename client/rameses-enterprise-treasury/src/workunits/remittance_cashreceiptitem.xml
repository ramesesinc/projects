<workunit extends="com/rameses/seti2/templates/CrudSubList.xml" 
          schemaName="remittance_cashreceiptitem" 
    cols="receiptno,receiptdate,item.title,amount"
    hiddenCols="receiptid" 
    allowDelete="false" allowCreate="false">
    
    <invokers>
        <invoker type="liquidation:section" caption="Receipt Details" action="init" /> 
        
        <invoker type="remittance:section" caption="Remittance Fund" tag="remittance" action="init" 
            cols="controlno,fund.title,totalcash,totalcheck,totalcr,amount"/>
        
        <invoker type="liquidation_fund:section" caption="Remittance Fund" tag="liquidation_fund" action="init" 
            cols="controlno,remittance.collector.name,totalcash,totalcheck,totalcr,amount"/>
        
    </invokers>
    <code>
        import  com.rameses.seti2.models.*;
        class LiquidationRemittanceList extends CrudSubListModel {
        
            public def getCustomFilter() {
                return [ "liquidationid=:pid", [ pid: masterEntity.objid ]  ];
            }
        
        }
    </code>
</workunit>
