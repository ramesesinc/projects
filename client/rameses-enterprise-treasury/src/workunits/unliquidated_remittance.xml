<workunit>
    
    <invokers>
        <invoker type="unliquidated_remittance:open" caption="Remittance" action="open" target="popup"/>

        <invoker type="formActions" caption="Close" name="_close" immediate="true" mnemonic="c" shortcut="ctrl C" icon="images/toolbars/cancel.png"/>
        <invoker type="formActions" caption="Print"  name="print" visibleWhen="#{mode == 'read' }" />
        <invoker type="formActions" caption="Reports" name="popupReports" visibleWhen="#{mode == 'read' }" category="remittance:reports" />
        <invoker type="formActions" caption="Approve" name="approve" visibleWhen="#{mode == 'read'}" mnemonic="e" index="100" role="LIQUIDATING_OFFICER"/>
        <invoker type="formActions" caption="Disapprove" name="disapprove" visibleWhen="#{mode == 'read' }" mnemonic="e" index="100" role="LIQUIDATING_OFFICER"/>
    
    </invokers>
   
    <code class="com.rameses.enterprise.treasury.models.CashReceiptRemittanceModel" />
   
    <pages>
        <page name="view" template="com.rameses.enterprise.treasury.views.RemittanceViewPage" />
    </pages> 
       
</workunit>
