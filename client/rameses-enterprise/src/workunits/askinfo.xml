        <invoker type="formActions" caption="Back" action="doBack" visibleWhen="#{hasBack == true}" mnemonic="B" immediate="true"/>
        <invoker type="formActions" caption="Next" action="doNext" mnemonic="N" shortcut="ctrl N"/>
    </invokers>
    <code class="com.rameses.enterprise.models.AskVariableInfoModel"/>
    <pages>
        <page template="com.rameses.enterprise.views.AskVariableInfoPage"/>
    </pages>
</workunit>