<workunit>
    <invokers>
        <invoker type="formActions" action="_close" caption="Close" 
                 shortcut="ctrl Q" mnemonic="q" />
        
        <invoker type="formActions" action="doNext" caption="Next" 
            icon="images/toolbars/arrow_right.png"  visibleWhen="#{ page == 'initial' }"/>
        
        
        <invoker type="formActions" action="doInitial" caption="Back" 
            icon="images/toolbars/arrow_right.png"  visibleWhen="#{ page == 'entry' }"/>
        
        <invoker type="formActions" action="save" caption="Save" 
            icon="images/toolbars/save.png"  visibleWhen="#{ page == 'entry' }"/>
       
    </invokers>
    <code class="com.rameses.treasury.common.models.CapturePaymentModel" />
    <pages>
        <page name="initial" template="com.rameses.treasury.common.views.CapturePaymentInitialPage"/>
        <page name="entry" template="com.rameses.treasury.common.views.CapturePaymentPage"/>
    </pages>
    
</workunit>