<workunit>
    <invokers>
        <invoker type="formActions" action="_close" caption="Close" 
                 shortcut="ctrl Q" mnemonic="q" />
        
        <invoker type="formActions" action="run" caption="Run" 
            icon="images/toolbars/arrow_right.png"  visibleWhen="#{ page == 'initial' }"/>
        
        
        <invoker type="formActions" action="save" caption="Save" 
            icon="images/toolbars/save.png"  visibleWhen="#{ page == 'entry' }"/>
       
    </invokers>
    <code class="com.rameses.treasury.common.models.MultiBillingModel" />
    <pages>
        <page name="initial" template="com.rameses.treasury.common.views.MultiBillingInitialPage"/>
        <page name="processing" template="com.rameses.treasury.common.views.ProcessingPage"/>
        <page name="completed" template="com.rameses.treasury.common.views.CompletedProcessingPage"/>
    </pages>
    
</workunit>