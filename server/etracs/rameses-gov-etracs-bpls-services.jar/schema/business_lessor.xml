		<field name="orgtype" />
		<field name="lessor_objid" />
		<field name="lessor_name" caption="Lessor Name" searchable="true"/>
		<field name="lessor_address_text" caption="Lessor Address"/>
		<field name="lessor_address_objid" />
		<field name="government" required="true"/>
		<field name="bldgno" caption="Bldg No" searchable="true" indexed="true"/>
		<field name="bldgname" caption="Bldg Name" searchable="true" indexed="true"/>
		<field name="street"  caption="Street" searchable="true" indexed="true"/>
		<field name="subdivision" caption="Subdivision" indexed="true"/>
		<field name="barangay_objid" />
		<field name="barangay_name" caption="Barangay" required="true" searchable="true" indexed="true"/>
