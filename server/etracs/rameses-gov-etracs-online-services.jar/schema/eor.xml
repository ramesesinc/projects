<schema adapter="eor">
	<element tablename="eor">
		<field name="objid" primary="true" prefix="EOR"/>
		<field name="receiptno" required="true" caption="Receipt No"/>
		<field name="receiptdate" required="true" type="date" caption="Receipt Date"/>
		<field name="txndate" required="true" type="timestamp"/>
		<field name="state" required="true"/>
		<field name="partnerid" required="true"/>
		<field name="txntype" required="true" caption="Txn Type"/>
		<field name="traceid" required="true"/>
		<field name="tracedate" required="true"/>
		<field name="refid" required="true"/>
		<field name="paidby" required="true"/>
		<field name="paidbyaddress" required="true"/>
		<field name="payer_objid" required="true"/>
		<!-- filipizen or kiosk -->
		<field name="paymethod" required="true"/>
		<field name="paymentrefid" required="true"/>
		<field name="remittanceid" required="true"/>
		<field name="remarks" required="true"/>
		<field name="amount" required="true" type="decimal"/>

		<complex name="items" ref="eor_item" jointype="one-to-many">
			<key field="objid" target="parentid" />
		</complex>	

		<complex name="sharing" ref="eor_share" jointype="one-to-many" required="false">
			<key field="objid" target="parentid" />
		</complex>	
	</element>
</schema>