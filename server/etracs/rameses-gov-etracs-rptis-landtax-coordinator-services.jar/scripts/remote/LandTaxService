import com.rameses.annotations.*
import com.rameses.util.*
import com.rameses.common.*
import com.rameses.services.extended.*

class LandTaxService  {
    @Service(value="LandTaxOnlineService", connection="local-etracs")
    def svc;

    @ProxyMethod
    public def getLedgerInfo( def params ) {
        return svc.getLedgerInfo(params)
    }
}
