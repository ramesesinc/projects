<schema>

	
	<element tablename="afreceiptitem" >
		<field name="objid" primary="true" prefix="AFRCTITM"/>			
		<field name="parentid" required="true"/>
		<field name="item_objid" required="true"/>
		<field name="item_title" required="true"/>
		<field name="unit" required="true"/>
		<field name="qtyrequested" defaultvalue="0" type="integer"/>
		<field name="qtyreceived" required="true" defaultvalue="0"  type="integer"/>
		<field name="remarks" />
		<field name="txntype" required="true"/>
		<field name="cost" />

		<complex name="item" ref="af" jointype="many-to-one" includefields="serieslength,denomination,formtype">
			<key field="item_objid" target="objid" />
		</complex>	
	</element>	

</schema>