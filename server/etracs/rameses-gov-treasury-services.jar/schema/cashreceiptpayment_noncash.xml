<schema>
	<element tablename="cashreceiptpayment_noncash">
		<field name="objid" primary="true" prefix="CRPMT"/>
		<field name="receiptid" required="true" />
		<field name="refid" />
		<field name="refno" required="true"/>
		<field name="refdate" required="true"/>
		<field name="reftype" />
		<field name="amount" />
		<field name="particulars" />
		<field name="fund_objid" required="true"/>

		<field name="remittancefundid"/>
		<field name="liquidationfundid"/>
		<field name="depositfundid"/>

		<complex name="receipt" ref="cashreceipt" jointype="many-to-one"  
			includefields="receiptno,receiptdate,collector.name" required="false">
			<key field="receiptid" target="objid" />
		</complex> 

		<complex name="fund" ref="fund" jointype="many-to-one"  includefields="objid,code,title">
			<key field="fund_objid" target="objid" />
		</complex>  

		<complex name="remittancefund" ref="remittance_fund" jointype="many-to-one"  
			includefields="remittanceid" required="false">
			<key field="remittancefundid" target="objid" />
		</complex> 

		<complex name="liquidationfund" ref="liquidation_fund" jointype="many-to-one"  
			includefields="liquidationid" required="false">
			<key field="liquidationfundid" target="objid" />
		</complex> 

	</element>
</schema>