<schema>
	<element  tablename="deposit_fund_transfer" >
		<field name="objid" primary="true" prefix="DEPFT" />

		<field name="depositid" required="true" />

		<complex name="fromfund" ref="fund" jointype="many-to-one" required="true">
			<key field="fromfundid" target="objid"/>
		</complex>	

		<complex name="tofund" ref="fund" jointype="many-to-one"  required="true">
			<key field="tofundid" target="objid"/>
		</complex>	

		<field name="amount" required="true" />
		<field name="amtused" required="true" />
	</element>
</schema>