import com.rameses.common.*
import com.rameses.annotations.*
import com.rameses.services.extended.*

class IndividualEntityLookupService extends ActiveListService {

	@ActiveDB(value='entity', em='marketbridge')
	def em

	@ProxyMethod
	public def getColumns(def o) {
		return [
			[name:"name", caption:"Name"], 
			[name:"address", caption:"Address"], 
			[name:"entityno", caption:"Entity No", width:80], 
			[name:"type", caption:"Type", maxWidth:100] 
		];	
	}

	
}