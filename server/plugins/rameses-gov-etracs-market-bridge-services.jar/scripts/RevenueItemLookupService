import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.common.*
import com.rameses.services.extended.*

class RevenueItemLookupService  extends ActiveListService {

	@ActiveDB(value="revenueitem", em="marketbridge")
	def em;

	@ProxyMethod
	public def getColumns( def o ) {
		return[
			[name:'code', caption:'Item Code', width:100, resizable:false ],
			[name:'title', caption:'Item Title' ],
			[name:'fund.code', caption:'Fund Code', width:50 ],
			[name:'fund.title', caption:'Fund Title' ],
		];
	}

	@ProxyMethod
	public def findAccount( def o ) {
		return em.findAccount( o );
	}

}
