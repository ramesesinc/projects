import com.rameses.common.*
import com.rameses.annotations.*
import com.rameses.services.extended.*

class MarketCashReceiptInterceptor {
	
	@After(pattern="MarketCashReceiptService.getBillItems")
	public void afterList(def evt) {
		def result = evt.result;
		def items = result.items.collect{ [
			acctid:it.item.objid, 
			acctno: it.item.code,
			accttitle: it.item.title,
			fundid: it.item.fund?.objid,
			fundname: it.item.fund?.title,
			amount: it.amount,
			remarks: it.remarks 
		]};
		result.items = items;
		result.items.each {
			println it;
		}
	}

	
}