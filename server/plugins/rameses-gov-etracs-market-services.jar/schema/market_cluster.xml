<schema  adapter="market">
	<element tablename="market_cluster">
		<field name="objid" primary="true" prefix="MKTCLU"/>
		<complex name="market" ref="market" jointype="many-to-one" required="false" 
			includefields="objid,code,name,address.text">
			<key field="marketid" target="objid" />
		</complex>
		<field name="name" required="true" caption="Cluster Name"/>
		<field name="description" />
	</element>
</schema>
