import com.rameses.annotations.*;
import java.text.*;
import com.rameses.functions.*;

class MarketAccountBillingReportService {
	
	@Service("MarketAccountBillingService")
	def billingSvc;
	
	@After(pattern="FormReportService.getData", eval="#{args[0].reportid == 'market_account_billing' }")
	public void getData( def evt ) {
		def p = evt.args[0];
		def result = evt.result;
		result.status = 0;
		result.data = billingSvc.getBilling( [acctid: p.parameters.acctid ] );
	}


}