<schema adapter="obo">
	<element tablename="building_permit_application">
		<field name="objid" primary="true"/>
		<field name="appno" caption="App No"/>
		<field name="appdate" caption="App Date"/>
		<field name="apptype" caption="App Type"/>
		<field name="appyear" caption="App Year"/>
		<field name="dtfiled" caption="Date Filed"/>
		<field name="filedby_name" caption="Filed By"/>
		<field name="owner_name" caption="Owner Name"/>
		<field name="owner_objid" visible="false"/>
		<field name="owner_address_text" caption="Owner Address"/>
		<field name="owner_address_objid" visible="false"/>

		<field name="description" caption="Description"/>
		<field name="costestimate" caption="Est. Cost"/>
		<field name="usetype" caption="Use Type"/>

		<field name="location_objid" caption="Barangay"/>
		<field name="barangay_name" caption="Barangay"/>

		<field name="particulars" required="true"/>
		<field name="billexpirydate" required="true"/>

	</element>	
</schema>