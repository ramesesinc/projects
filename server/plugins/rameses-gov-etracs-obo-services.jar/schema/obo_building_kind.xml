<schema adapter="obodb">

	<element tablename="obo_building_kind">
		<field name="objid" primary="true" prefix="OCCTYPE"/>
		<field name="name" required="true" style="code" caption="Name" editable="false"/>
		<complex name="classification" ref="obo_use_classification" ref="many-to-one" includefields="name">
			<key field="classification_objid" target="objid" />
		</complex>
		<complex name="type" ref="obo_building_type" jointype="many-to-one" includefields="name,groupid,division">
			<key field="type_objid" target="objid" />
		</complex>
	</element>

</schema>
		