<schema adapter="obodb">

	<element tablename="obovariable">
		<field name="objid" primary="true"/>
		<field name="state" required="true" visible="false"/>
		<field name="name" required="true" style="code" editable="false"/>
		<field name="datatype" required="true"/>
		<field name="caption" required="true"/>
		<field name="description" required="true"/>
		<field name="arrayvalues" required="false" visible="false"/>
		<field name="system" required="false" defaultValue="0"/>
		<field name="sortorder" required="true" visible="false"/>
		<field name="category" required="true"/>
		<field name="section" required="true"/>
	</element>

</schema>
		