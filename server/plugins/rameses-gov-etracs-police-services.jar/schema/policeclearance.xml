<schema adapter="police">
	<element tablename="policeclearance" >
		<field name="objid" required="true" prefix="PISC"/>
		<field name="state" required="true"/>
		<field name="dtfiled" required="true"/>
		<field name="controlno" required="true"/>
		<field name="dtissued" required="true"/>
		<field name="issuedby_objid" required="true"/>
		<field name="issuedby_name" required="true"/>
		<field name="dtexpiry" required="true"/>

		<field name="appid" required="true"/>		
		<field name="txnyear" required="true"/> 

		<field name="ctc_refno"/> 
		<field name="ctc_issuedat"/> 
		<field name="ctc_issuedon"/> 
		<field name="withcase"/> 
		<field name="remarks"/> 
		
	</element>
</schema>