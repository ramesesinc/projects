<schema adapter="vehicledb">
	<element tablename="vehicle_fishboat" >
		<field name="objid" primary="true" prefix="VFB"/>

		<field name="vesselname" required="true"/>
		<field name="length" required="true" type="decimal"/>
		<field name="breadth" required="true" type="decimal"/>
		<field name="depth" required="true" type="decimal"/>
		<field name="grosstonnage" required="true" type="decimal"/>
		<field name="nettonnage" required="true" type="decimal"/>
		<field name="buildername" required="true"/>
		<field name="builderplace" required="true"/>
		<field name="buildyear" required="true" type="integer"/>

		<field name="createdby_objid" required="true" />
		<field name="createdby_name" required="true" caption="Created By"/>
		<field name="dtcreated" required="true" caption="Date Created"/>

		<complex name="franchise" ref="vehicle_franchise" jointype="many-to-one" required="false"
			includefields="controlno,state">
			<key field="controlid" target="objid" />	
		</complex>  
		
		<complex name="app" ref="vehicle_application" jointype="many-to-one" required="false"
			includefields="appno">
			<key field="appid" target="objid"/>
		</complex>	

		<complex name="engines" ref="vehicle_fishboat_engine" jointype="one-to-many">
			<key field="objid" target="parentid"/>
		</complex> 

	</element>
</schema>