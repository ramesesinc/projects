<schema adapter="vehicledb">
	<element tablename="vehicle_franchise_fishboat">
		<field name="objid" primary="true"/>
		<field name="vehicleid" visible="false" />
		<field name="state"  caption="Status"/>
		<field name="vehicletype" />
		<field name="controlno" caption="Control No"/>
		<field name="expirydate" caption="Expiry Date"/>
		<field name="activeyear" caption="Active Year"/>
		<field name="vehicle_objid" />

		<field name="vehicle_vesselname"/>
		<field name="vehicle_length" type="decimal"/>
		<field name="vehicle_breadth" type="decimal"/>
		<field name="vehicle_depth"  type="decimal"/>
		<field name="vehicle_grosstonnage"  type="decimal"/>
		<field name="vehicle_nettonnage" type="decimal"/>
		<field name="vehicle_buildername" />
		<field name="vehicle_builderplace"/>
