<schema adapter="vehicledb">
	<element tablename="vehicle">
		<field name="objid" primary="true"  prefix="VACCT"/>
		<field name="txntype" required="true"/>
		<field name="particulars" required="true"/>

		<field name="acctno" required="true" caption="Acct No"/>
		<field name="state" required="true" />
		<field name="activeyear" required="true" type="integer"/>

		<field name="dtregistered" required="true" />
		<field name="createdby_objid" required="true" />
		<field name="createdby_name" required="true" />
		<field name="expirydate" required="true" />

		<field name="owner_objid" required="true" />
		<field name="owner_name" required="true" />
		<field name="owner_address_text" required="true" />
		<field name="owner_address_objid" required="true" />
		<field name="barangay_objid" required="true" />
		<field name="barangay_name" required="true" />

		<complex name="control" ref="vehicle_control" jointype="many-to-one" includefields="controlno" required="true">
			<key field="controlid" target="objid" />
		</complex> 

	</element>
</schema>