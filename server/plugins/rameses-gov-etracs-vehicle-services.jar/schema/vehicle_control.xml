<schema adapter="vehicledb">
	<element tablename="vehicle_control">
		<field name="objid" primary="true" prefix="MF"/>
		<field name="controlno"  caption="Control No" required="true" searchable="true" indexed="true"/>
		<field name="vehicletype"  required="true" />
		<complex name="vehicle" ref="vehicle_account" jointype="many-to-one" 
			includefields="owner.name,owner.address.*,particulars,activeyear,state" required="false">
			<key field="vehicleid"  target="objid" />
		</complex>
	</element>
</schema>