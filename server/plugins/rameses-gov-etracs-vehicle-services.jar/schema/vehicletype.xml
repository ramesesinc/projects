<schema adapter="vehicledb">
	<element tablename="vehicletype">
		<field name="objid" primary="true" visible="true" editable="false" caption="Name"/>
		<field name="title" required="true" caption="Title"/>
		
		<field name="controlpattern" required="true" />
		<field name="appnopattern" required="true" />
		<field name="permitnopattern" required="true" />

		<field name="issued" type="integer" required="true" defaultValue="0"/>
		<field name="icon" /> 
		<field name="allowedfields" required="true"/> 
		<field name="renewalfrequency" required="true" caption="Renewal Frequency" defaultValue="YEAR"/> 
	</element>
</schema>