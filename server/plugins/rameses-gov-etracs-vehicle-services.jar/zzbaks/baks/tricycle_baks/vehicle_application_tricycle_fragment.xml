<schema adapter="vehicledb">
	<element tablename="vehicle_application_tricycle_fragment">
		<field name="objid" primary="true"/>

		<field name="appno" caption="App No"/>
		<field name="appdate" caption="App Date"/>
		<field name="apptype" caption="App Type"/>
		<field name="txnmode" />
		<field name="appyear" />
		<field name="dtfiled" />
		<field name="filedby_objid" />
		<field name="filedby_name" />
		<field name="particulars"  />

		<field name="owner_objid" />
		<field name="owner_name" caption="Owner Name"/>
		<field name="owner_address_text" />
		<field name="owner_address_objid" />
		<field name="barangay_objid"  />
		<field name="barangay_name"  />

		<field name="plateno" caption="Plate No" searchable="true"  indexed="true"/>
		<field name="engineno" caption="Engine No" searchable="true" indexed="true"/>
		<field name="bodyno" caption="Body No" searchable="true" indexed="true"/>

		<field name="sidecarno" caption="Sidecar No"  indexed="true"/>
		<field name="make" caption="Make"/>
		<field name="model" caption="Model"/>
		<field name="color" caption="Color"/>

		<field name="chassisno" caption="Chassis No"/>
		<field name="sidecarcolor" caption="Color"/>
		<field name="crname" caption="CR Name"/>
		<field name="franchise_objid"  visible="false"/>
		<field name="franchise_controlno"  caption="Control No"/>
	</element>	
</schema>