<schema adapter="vehicledb">
	<element tablename="vehicle_account_boat_engine">
		<field name="objid" primary="true" prefix="BOATACCTENG" />

		<complex name="account" ref="vehicle_account" jointype="many-to-one" includefields="acctno,name" required="false">
			<key field="acctid" target="objid"/>		
		</complex>	

		<field name="make"  />
		<field name="serialno"  />
		<field name="horsepower"/>

	</element>
</schema>