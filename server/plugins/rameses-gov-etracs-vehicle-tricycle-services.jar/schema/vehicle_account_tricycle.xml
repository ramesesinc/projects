<schema adapter="vehicledb">
	<element tablename="vehicle_account_tricycle" extends="vehicle_account">
		<field name="objid" primary="true" prefix="VMTOP"/>
		<field name="plateno" required="true"/>
		<field name="engineno" required="true"/>
		<field name="bodyno" required="true"/>
		<field name="sidecarno" required="false"/>
		<field name="make" required="false"/>
		<field name="model" required="false"/>
		<field name="color" required="false"/>
	</element>
</schema>