		<field name="barangay_name" caption="Barangay"/>
		<field name="billexpirydate" required="true"/>
		<field name="vehicle_plateno" caption="Plate No"/>
		<field name="vehicle_engineno" caption="Engine No"/>
		<field name="vehicle_bodyno" caption="Body No"/>

		<field name="vehicle_sidecarno" caption="Sidecar No"/>
