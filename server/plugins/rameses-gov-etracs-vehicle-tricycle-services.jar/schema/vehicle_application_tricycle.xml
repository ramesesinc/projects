<schema adapter="vehicledb">
	<element tablename="vehicle_application_tricycle" extends="vehicle_application">
		<field name="objid" primary="true" prefix="MTOPAPP" />

		<complex name="vehicle" ref="vehicle_account_tricycle" jointype="many-to-one" required="true" 
			includefields="plateno,engineno,bodyno,sidecarno,make,model,color">
			<key field="vehicleid" target="objid" />
		</complex>
		
		<complex name="fees" ref="vehicle_application_fee" jointype="one-to-many">
			<key field="objid" target="appid" />
		</complex>

	</element>
</schema>