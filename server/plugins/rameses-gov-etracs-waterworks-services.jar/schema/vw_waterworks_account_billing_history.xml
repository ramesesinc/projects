<schema adapter="waterworks">
	<element tablename="vw_waterworks_account_billing_history">
		<field name="billid" primary="true"/> 
		<field name="acctid" /> 
		<field name="acctno" caption="AcctNo"/> 
		<field name="year" caption="Year" type="integer" indexed="true" searchable="true"/> 
		<field name="month" caption="Month"/> 		
		<field name="periodindexno" /> 
		<field name="readingdate" caption="ReadingDate" type="date"/> 
		<field name="prevreading" caption="PrevReading" type="integer"/> 
		<field name="reading" caption="CurrentReading" type="integer"/> 
		<field name="volume" caption="Volume" type="integer"/> 
		<field name="amount" caption="Amount" type="decimal"/> 
		<field name="arrears" caption="Arrears" type="decimal"/> 
		<field name="surcharge" caption="Surcharge" type="decimal"/> 
		<field name="interest" caption="Interest" type="decimal"/> 
		<field name="credits" caption="Credits" type="decimal"/> 
		<field name="consumptionid" /> 
		<field name="batchid" /> 
		<field name="scheduleid" /> 
		<field name="zoneid" /> 
		<field name="meterid" /> 

		<field name="monthname" expr="MONTH_ARRAY( month )" caption="MonthName"/> 
	</element>
</schema>