import com.rameses.annotations.*;

class WaterworksCashReceiptBarcodeServiceInterceptor{
	
	@DataContext("waterworks_account_ledger")
	def ledger;

	@Before(pattern="CashReceiptBarcodeService.init", eval="#{args[0].prefix == null}")
	public void supplyWaterworksPrefix(def evt) {
		def o = evt.args[0];
		o.prefix = "51030";
	}


}