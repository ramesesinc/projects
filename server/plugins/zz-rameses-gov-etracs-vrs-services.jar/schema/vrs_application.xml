		<field name="owner_name" required="true"/>
		<field name="owner_address_text" required="true"/>
		<field name="vehicle_make" required="true"/>
		<field name="vehicle_motorno" required="true"/>
		<field name="vehicle_chassisno" required="true"/>
		<field name="vehicle_plateno" required="true"/>
		<field name="remarks" />
