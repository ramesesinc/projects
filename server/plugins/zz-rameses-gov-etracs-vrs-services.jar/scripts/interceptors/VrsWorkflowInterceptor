import com.rameses.annotations.*;
import java.rmi.server.UID;

class VrsWorkflowInterceptor {
	
	@After(pattern="SetiWorkflowService.getAssigneeList",eval="#{args[0].processname=='mtop'}")
	public void addAssignees(def evt) {
		def p = evt.args[0];
		def result = evt.result;
		if( p.to == 'approval') {
			result.add( [objid:'P1', name:'ELMO NAZARENO'] );
			result.add( [objid:'P2', name:'WORGIE FLORES'] );
		}
	}

}