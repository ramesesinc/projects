<schema adapter="waterworks">
	<element tablename="waterworks_application">
		<field name="objid" primary="true"  prefix="WAPP"/>
		<field name="state" required="true" visible="false" defaultValue="ACTIVE"/>
		<field name="dtfiled" required="false" indexed="true" type="date" caption="Date Filed"/>
		<field name="appno" required="true" indexed="true" caption="App No"/>
		<complex name="account" ref="waterworks_account" jointype="many-to-one">
			<key field="acctid" target="objid"/> 
		</complex>
	</element>

</schema>