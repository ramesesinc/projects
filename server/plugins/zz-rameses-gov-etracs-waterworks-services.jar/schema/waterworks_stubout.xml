<schema adapter="waterworks">

	<element tablename="waterworks_stubout">
		<field name="objid" primary="true" prefix="SO"/>
		<field name="title" required="true" indexed="true" caption="Title"/>
		<field name="description" required="true" indexed="true" caption="Description"/>
		<complex name="readinggroup" ref="waterworks_readinggroup" jointype="many-to-one" required="true" caption="Reading Group">
			<key field="readinggroup_objid" target="objid"/> 
		</complex> 
	</element>
</schema>