<schema adapter="waterworks">

	<element tablename="waterworks_stubout">
		<field name="objid" primary="true" prefix="SO"/>
		<field name="code" required="true" indexed="true" caption="Code" searchable="true"/>
		<field name="description" required="true" indexed="true" caption="Description"/>
		<complex name="zone" ref="waterworks_sector_zone" jointype="many-to-one" required="false">
			<key field="zoneid" target="objid"/>	
		</complex>	
	</element>

</schema>