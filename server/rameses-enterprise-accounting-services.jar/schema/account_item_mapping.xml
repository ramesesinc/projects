
<schema>

	<element tablename="account_item_mapping">
		<field name="objid" primary="true" prefix="ACCTMAP"/>

		<complex name="maingroup" ref="account_maingroup" jointype="many-to-one" includefields="objid,name,title">
			<key field="maingroupid" target="objid" />	
		</complex> 

		<complex name="account" ref="account" jointype="many-to-one" includefields="objid,code,title,headerid" >
			<key field="acctid" target="objid" />	
		</complex> 

		<complex name="item" ref="itemaccount" jointype="many-to-one" includefields="objid,code,title" >
			<key field="itemid" target="objid" />	
		</complex> 
	</element>

</schema>