<schema>
	<element tablename="account_receivable">
		<field name="objid" primary="true" prefix="AR"/>
		<field name="code" required="true" caption="Acct Code"/>
		<field name="title" required="true" caption="Acct Title"/>

		<complex name="account" ref="itemaccount" jointype="many-to-one" includefields="objid,code,title" required="false"> 
			<key field="acctid" target="objid"/> 
		</complex>
		
	</element>
</schema>

