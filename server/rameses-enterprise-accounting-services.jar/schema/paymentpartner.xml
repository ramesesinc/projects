<schema>
	<element tablename="paymentpartner">
		<field name="objid" primary="true" prefix="PMTPRT"/>
		<field name="code" required="true" />
		<field name="name" required="true" />
		<complex name="receivableaccount" ref="itemaccount" jointype="many-to-one" includefields="objid,code,title" required="false">
			<key field="receivableacctid" target="objid" />
		</complex>	
	</element>
</schema>	