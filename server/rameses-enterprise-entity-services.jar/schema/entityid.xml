<schema adapter="main">

	<element tablename="entityid">
		<field name="objid" primary="true" prefix="EID"/>
		<field name="entityid" required="true"/>
		<complex name="entity" ref="entityindividual" required="true" jointype="many-to-one"
			includefields="entityno,lastname,firstname">
			<key field="entityid" target="objid"/>
		</complex>	
		<field name="idtype" required="true"/>
		<field name="idno" required="true"/>
		<field name="dtissued" required="true"/>
		<field name="dtexpiry" required="true"/>
	</element> 

</schema>
