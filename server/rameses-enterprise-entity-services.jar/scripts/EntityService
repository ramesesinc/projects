import com.rameses.annotations.*
import com.rameses.common.*
import java.rmi.server.UID;
import com.rameses.util.*;
import com.rameses.io.*;

class EntityService {

	@DataContext("entity")
	def	em; 

	@ProxyMethod
	public def open(o) {
		return em.select("objid,entityno,entityname,address.text,address.objid,address.barangay.objid,address.barangay.name").find([objid: o.objid]).first(); 	
	}


}
