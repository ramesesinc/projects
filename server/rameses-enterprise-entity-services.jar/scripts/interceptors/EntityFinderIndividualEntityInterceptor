import com.rameses.annotations.*
import com.rameses.common.*
import java.rmi.server.UID;
import com.rameses.util.*;
import com.rameses.io.*;


class EntityFinderIndividualEntityInterceptor  {

	@DataContext("entityindividual")
	def	em; 

	@After(pattern="EntityFinderService.find", eval="#{result.type == 'INDIVIDUAL'}")
	public void loadIndividualInfoForFind(def evt ) {
		def result = evt.result;
		def z= em.find( [objid: result.objid] ).first();
		result.putAll( z );
	}

}
