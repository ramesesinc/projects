<schema>
	<element tablename="jev">
		<field name="objid" primary="true"/>
		<field name="jevno" caption="Jev No"/>
		<field name="jevdate" caption="Jev Date"/>
		<field name="dtposted" required="true" caption="Date Posted" type="date"/>
		<field name="txntype" required="true" caption="Txn Type"/>
		<field name="refid" required="true"/>
		<field name="refno" required="true" caption="Ref No"/>
		<field name="reftype" required="true" caption="Ref Type"/>
		<field name="refdate" required="true" caption="Ref Date" type="date"/>
		<field name="amount" required="true" caption="Amount" type="decimal"/>
		<field name="state" required="true"/>
		<field name="postedby_objid" required="true"/>
		<field name="postedby_name" required="true"/>
		<field name="verifiedby_objid" />
		<field name="verifiedby_name" />
		<field name="dtverified" type="date"/>

		<!-- items -->
		<complex name="items" ref="jevfund" jointype="one-to-many">
			<key field="objid" target="jevid" />
		</complex>
	</element>
</schema>