<schema>

	<element tablename="jevfund">
		<field name="objid" primary="true" prefix="JEVFND"/>
		<field name="jevid" />

		<complex name="jev" ref="jev" jointype="many-to-one" includefields="refno,refdate,txntype">
			<key field="jevid" target="objid" />
		</complex>

		<field name="amount" type="decimal" caption="Amount"/>
		<complex name="fund" ref="fund" jointype="many-to-one" includefields="objid,code,title" required="false">
			<key field="fundid" target="objid" />
		</complex>

		<!--
		<complex name="items" ref="jevitem" jointype="one-to-many">
			<key field="objid" target="jevfundid" />
		</complex>
		-->
		
	</element>

</schema>