<schema>

	<element tablename="cashreceipt_share">
		<field name="objid" primary="true" prefix="RCTSHR"/>
		<field name="receiptid" required="true"/>
		<complex name="refaccount" ref="itemaccount" jointype="many-to-one" required="true">
			<key field="refacctid" target="objid"/> 
		</complex>
		<complex name="payableaccount" ref="itemaccount" jointype="many-to-one" required="false">
			<key field="payableacctid" target="objid"/> 
		</complex>

		<field name="amount" required="true" type="decimal"/>
	</element>

</schema>