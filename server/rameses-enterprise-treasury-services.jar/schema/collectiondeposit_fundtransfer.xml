<schema>

	<element  tablename="collectiondeposit_fundtransfer">
		<field name="objid" primary="true"/>

		<complex name="deposit" ref="collectiondeposit" jointype="many-to-one" includefields="controlno,dtfiled">
			<key field="depositid" target="objid" />
		</complex>	


		<field name="state" required="true"/>
		<field name="fund_objid" required="true"/>
		<field name="bankaccountid" required="true"/>
		<field name="amount" required="true" type="decimal"/>
		<field name="amounttransferred" required="true" type="decimal"/>

		<complex name="bankaccount" ref="bankaccount" jointype="many-to-one" includefields="code,title">
			<key field="bankaccountid" target="objid" />
		</complex>	

		<complex name="fund" ref="fund" jointype="many-to-one" required="false" includefields="objid,code,title">
			<key field="fund_objid" target="objid" />
		</complex>  
	</element>

</schema>


