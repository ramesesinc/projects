<schema>
	<element name="liquidation" tablename="liquidation" >
		<field name="objid" primary="true" prefix="LIQ"/>
		<field name="state" required="true"/>
		<field name="txnno" required="true" searchable="true"/>
		<field name="txndate" required="true" type="date"/>
		<field name="dtposted" required="true" type="date" indexed="true" caption="Date"/>

		<field name="liquidatingofficer_objid" required="true"/>
		<field name="liquidatingofficer_name" required="true" searchable="true"/>
		<field name="liquidatingofficer_title" required="true"/>
		<field name="liquidatingofficer_signature" />

		<field name="amount" required="true"/>
		<field name="totalcash" required="true"/>
		<field name="totalnoncash" required="true"/>
		<complex name="cashbreakdown" required="true" serializer="default" type="list"/>
		<field name="posted" />

		<field name="cashier_objid" />
		<field name="cashier_name" />
		<field name="cashier_title"/>
		<field name="cashier_signature"/>

	</element>

	<element name="remittance" tablename="liquidation_remittance">
		<field name="objid" primary="true"/>
		<field name="liquidationid" required="true"/>
	</element>

	<element name="checkpayment" tablename="liquidation_noncashpayment">
		<field name="objid" primary="true"/>
		<field name="liquidationid" required="true"/>
		<field name="liquidationfundid" required="true"/>
	</element>

	<element name="cashier_fund" tablename="liquidation_fund">
		<field name="objid" primary="true"/>
		<field name="liquidationid" required="true"/>
		<field name="fund_objid" required="true"/>
		<field name="fund_title" required="true"/>
		<field name="cashier_objid" />
		<field name="cashier_name" />
		<field name="amount" />
	</element>

</schema>	