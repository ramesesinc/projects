<schema>

	<element  tablename="liquidation_fund">
		<field name="objid" primary="true"/>
		<field name="controlno" />
		<field name="liquidationid" required="true"/>
		<field name="fund_objid" required="true"/>
		<field name="fund_title" required="true"/>
		<field name="cashier_objid" />
		<field name="cashier_name" />
		<field name="amount" type="decimal"/>

		<field name="totalcash" type="decimal"/>
		<field name="totalnoncash" type="decimal"/>
		<field name="totalcr" type="decimal"/>

		<complex name="liquidation" ref="liquidation" jointype="many-to-one" required="false" includefields="objid,txnno,dtposted,state">
			<key field="liquidationid" target="objid" />
		</complex>	

		<complex name="fund" ref="fund" jointype="many-to-one" required="false" includefields="objid,code,title">
			<key field="fund_objid" target="objid" />
		</complex>  

	</element>

</schema>	