<schema>

	<element name="liquidation_fund" tablename="liquidation_fund">
		<field name="objid" primary="true"/>
		<field name="liquidationid" required="true"/>
		<field name="fund_objid" required="true"/>
		<field name="fund_title" required="true"/>
		<field name="cashier_objid" />
		<field name="cashier_name" />
		<field name="amount" />

		<complex name="liquidation" ref="liquidation" jointype="many-to-one" required="false" includefields="objid,txnno,dtposted,state">
			<key field="liquidationid" target="objid" />
		</complex>	
		
	</element>

</schema>	