<schema>
	<element tablename="paymentorder">
		<field name="txnid" primary="true"/>
		<field name="txndate" type="timestamp" />
		<field name="payer_objid" />
		<field name="payer_name" />
		<field name="paidby" />
		<field name="paidbyaddress" />
		<field name="amount" type="decimal" />
		<field name="collectiontypehandler" />
		<field name="expirydate" type="timestamp"/>
		<field name="refid" />
		<field name="refno" />
		<complex name="info" serializer="default" required="true"/>
		<complex name="txntype" ref="paymentorder_type" jointype="many-to-one">
			<key field="txntypeid" target="objid"/>
		</complex>
	</element>

</schema>