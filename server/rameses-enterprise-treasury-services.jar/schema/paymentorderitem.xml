<schema>
	<element tablename="paymentorderitem">
		<field name="objid" primary="true"/>
		<field name="parentid" />
		<complex name="parent" ref="paymentorder" jointype="many-to-one"  required="true">
			<key field="parentid" target="objid"/>
		</complex>	
		<field name="item_objid" />
		<field name="item_code" />
		<field name="item_title" />
		<field name="amount"  type="decimal"/>
		<field name="remarks" />
	</element>
</schema>
