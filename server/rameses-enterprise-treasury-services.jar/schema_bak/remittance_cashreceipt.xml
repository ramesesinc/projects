<schema>
	<element tablename="remittance_cashreceipt">
		<field name="objid" primary="true"/>
		<field name="remittanceid" required="true"/> 

		<complex name="remittance" ref="remittance" jointype="many-to-one" includefields="objid,txnno,dtposted,liquidatingofficer.name" required="false">
			<key field="remittanceid" target="objid" />
		</complex>

		<complex name="cashreceipt" ref="cashreceipt" jointype="many-to-one"  required="false"
			includefields="formno,receiptno,receiptdate,amount,paidby,subcollector.name,totalcash,totalnoncash,totalcredit">
			<key field="objid" target="objid" />
		</complex>

		<complex name="cashreceiptvoid" ref="cashreceipt_void" jointype="many-to-one" includefields="objid" required="false">
			<key field="objid" target="receiptid" />
		</complex>
	</element>
</schema>