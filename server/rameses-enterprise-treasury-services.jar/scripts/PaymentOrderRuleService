import com.rameses.annotations.*;
import com.rameses.rules.common.*;


class PaymentOrderService  {

	@Service("RuleExecutionService")
	def ruleSvc;

	@ProxyMethod
	public def execute( def p ) {

		def facts = [];

		def h = { o->
			if(o == 'precomputefee' ) {
				throw new BreakException();
			}
		}
		RuleAction ra = new RulAction();
		ra.addCommand( "set-duedate", new SetDueDate()); 
		ra.addCommand( "compute-fee", new ComputeFee()); 


		ruleSvc.execute( "paymentorder", facts, actions, h );

	}

}
