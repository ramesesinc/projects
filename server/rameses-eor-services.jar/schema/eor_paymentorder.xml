<schema>
	<element tablename="eor_paymentorder">
		<field name="objid" primary="true"/>
		<field name="traceno" />
		<field name="tracedate" />
		<field name="dtcreated" />
		<field name="dtposted" />

		<field name="paymentorder_txnid" required="true"/>
		<field name="paymentorder_txndate" type="timestamp" required="true" />
		<field name="paymentorder_controlno" required="true" />
		<field name="paymentorder_payer_objid" required="true" />
		<field name="paymentorder_payer_name" required="true" />
		<field name="paymentorder_paidby" required="true" />
		<field name="paymentorder_paidbyaddress" required="true" />
		<field name="paymentorder_particulars" required="true" />
		<field name="paymentorder_amount" type="decimal" required="true" />
		<field name="paymentorder_expirydate" type="date" required="true" />
		<field name="paymentorder_refid" required="true"  />
		<field name="paymentorder_refno" />
		<complex name="paymentorder_info" serializer="default" required="true"/>
		<complex name="paymentorder_txntype" ref="paymentorder_type" jointype="many-to-one">
			<key field="paymentorder_txntypeid" target="objid"/>
		</complex>
	</element>
</schema>	

