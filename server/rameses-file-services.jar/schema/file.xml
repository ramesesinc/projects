<schema>

	<element name="file" tablename="sys_file_header"> 
		<field name="objid" primary="true"/> 
		<field name="state" required="true"/> 
		<field name="dtfiled" required="true"/> 
		<field name="filename" required="true"/> 
		<field name="filetype" required="true"/> 
		<field name="filesize" required="true"/> 
		<field name="chunkcount" required="true"/> 
		<field name="info" /> 
	</element>

	<element name="detail" tablename="sys_file_detail">
		<field name="objid" required="true"/> 
		<field name="parentid" required="true"/> 
		<field name="indexno" required="true"/> 
		<field name="content" required="true"/> 
	</element>

</schema>
