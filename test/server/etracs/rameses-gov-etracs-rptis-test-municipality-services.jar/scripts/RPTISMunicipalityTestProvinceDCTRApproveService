import com.rameses.annotations.*;
import com.rameses.util.*;
import java.rmi.server.*;
import com.rameses.services.extended.*;

public class RPTISMunicipalityTestProvinceDCTRApproveService
{
	@Env 
	def env 

	@ActiveDB('rptis_test_faas')
	def em 

	@Service('FAASService')
	def faasSvc 

	@Service('RPTLedgerService')
	def ledgerSvc 

	@Service('RPTISUnitTestService')
	def test

	@Service('RPTISTestFAASOnlineService')
	def faasOnlineTest


	@Service('RPTISTestLedgerService')
	def ledgerTest


	@ProxyMethod
	public def testProvinceTransferredFaas( faas ) {
		faasOnlineTest.testApprovedOnlineFaas(faas)
		return '[municipality] Province Simple Transferred FAAS tested'
	}

	@ProxyMethod
	public def testProvinceTransferredLedger( faas ) {
		faasOnlineTest.testApprovedOnlineLedgerFromFaas(faas)
        return '[municipality] Province Simple Transferred Ledger tested'
	}


}